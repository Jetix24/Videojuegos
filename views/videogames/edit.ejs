 <!-- Titulo de la interfaz-->
<h2 class="page-header">Editar Videojuego</h2>
 <!-- Renderiza el formulario de videojuegos y crea botones para cancelar o actualizar-->
<form action="/videogames/<%= videogame.id %>?_method=PUT" method="POST">
  <%- include('_form_fields') %>
  <div class="form-row form-row-end btn-row">
    <a class="btn btn-danger" href="/videogames">Cancelar</a>
    <button class="btn btn-primary" type="submit">Actualizar</button>
  </div>
</form>