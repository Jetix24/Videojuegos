<h2 class="page-header">Buscar libros</h2>
<form action="/books" method="GET">
    <div>
        <label>Titulo</label>
        <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>
    <div>
        <label>Publish After</label>
        <input type="date" name="publishedAfter" value="<%= searchOptions.publishAfter %>">
    </div>
    <div>
        <label>Publish Before</label>
        <input type="date" name="publishedBefore" value="<%= searchOptions.publishBefore %>">
    </div> 
    <button type="submit">Search</button>
</form>
<div>
    <% books.forEach(book => { %>
        <a href="/books/<%= book.id %>">
            <img height="300" width="200" src="<%= book.coverImagePath %>">
          </a>
    <% }) %>
</div>